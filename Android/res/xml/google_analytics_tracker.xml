<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:tools="http://schemas.android.com/tools"
           tools:ignore="TypographyDashes">

    <!-- Google Analytics tracking id -->
    <string name="ga_trackingId">UA-51862927-1</string>

    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!-- Enable uncaught exception tracking -->
    <bool name="ga_reportUncaughtExceptions">true</bool>

    <!-- The screen names that will show up in the report -->
    <screenName name="org.droidplanner.android.activities.FlightActivity">
        Flight Screen
    </screenName>

    <screenName name="org.droidplanner.android.activities.EditorActivity">
        Editor Screen
    </screenName>

    <screenName name="org.droidplanner.android.activities.ConfigurationActivity">
        Configuration Screen
    </screenName>

    <screenName name="org.droidplanner.android.activities.SettingsActivity">
        Settings Screen
    </screenName>
</resources>